{
    "name": "pinfirestudios/yii2-oauth2-authorization-server",
    "description": "OAuth2 Authorization Server for Yii2.  Fork of abandoned filsh/yii2-oauth2-server package",
    "keywords": ["yii", "extension", "module", "oauth", "oauth2"],
    "homepage": "https://github.com/pinfirestudios/yii2-oauth2-authorization-server",
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "email": "imaliy.filsh@gmail.com",
        "source": "https://github.com/filsh/yii2-oauth2-server"
    },
    "authors": [
        {
            "name": "Igor Maliy",
            "email": "imaliy.filsh@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "require": {
        "yiisoft/yii2": "*",
        "bshaffer/oauth2-server-php": "~1.11"
    },
    "autoload": {
        "psr-4": {
            "filsh\\yii2\\oauth2server\\": ""
        }
    },
    "extra": {
        "bootstrap": "filsh\\yii2\\oauth2server\\Bootstrap",
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
